{
  "name": "warning-message",
  "version": "1.1.0",
  "description": "A declarative way to return warning messages",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "lint": "eslint index.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run babel",
    "babel": "babel src --out-dir lib",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudolfsonjunior/node-warning-message.git"
  },
  "keywords": [
    "warning",
    "message",
    "warnings",
    "input",
    "form",
    "warning-message"
  ],
  "license": "MIT",
  "author": "Lukas Aichbauer <rudolfson.junior@gmail.com>",
  "bugs": {
    "url": "https://github.com/rudolfsonjunior/node-warning-message/issues"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "homepage": "https://github.com/rudolfsonjunior/node-warning-message#readme",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.2.2",
    "coveralls": "^2.12.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.2",
    "nyc": "^10.1.2"
  }
}
